<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context="com.koo.hanmo.androidsampletest.UiFragmentActivity">

    <FrameLayout
        android:id="@+id/frame_uifragment_1"
        android:layout_width="400px"
        android:layout_height="400px"
        ></FrameLayout>

    <FrameLayout
        android:id="@+id/frame_uifragment_2"
        android:layout_gravity="right"
        android:layout_width="400px"
        android:layout_height="400px"></FrameLayout>

    <Button
        android:id="@+id/btn_uifragment_change"
        android:text="change"
        android:textSize="20dp"
        android:layout_gravity="center_horizontal"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content" />

</LinearLayout>
